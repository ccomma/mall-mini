{
  "pages": [
    "pages/home/home",
    "pages/category/category",
    "pages/new-item/new-item",
    "pages/cart/cart",
    "pages/my/my",
    "pages/item-detail/item-detail"
  ],
  "window": {
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#f5f5f5",
    "navigationBarTextStyle": "black",
    "onReachBottomDistance": 50
  },
  "tabBar": {
    "color": "#2c2c2c",
    "selectedColor": "#d81e06",
    "list": [
      {
        "text": "首页",
        "pagePath": "pages/home/home",
        "iconPath": "static/img/home.png",
        "selectedIconPath": "static/img/home_fill.png"
      },
      {
        "text": "分类",
        "pagePath": "pages/category/category",
        "iconPath": "static/img/all.png",
        "selectedIconPath": "static/img/all_fill.png"
      },
      {
        "text": "新品",
        "pagePath": "pages/new-item/new-item",
        "iconPath": "static/img/discover.png",
        "selectedIconPath": "static/img/discover_fill.png"
      },
      {
        "text": "购物车",
        "pagePath": "pages/cart/cart",
        "iconPath": "static/img/cart.png",
        "selectedIconPath": "static/img/cart_fill.png"
      },
      {
        "text": "我的",
        "pagePath": "pages/my/my",
        "iconPath": "static/img/people.png",
        "selectedIconPath": "static/img/people_fill.png"
      }
    ]
  },
  "lazyCodeLoading": "requiredComponents",
  "style": "v2",
  "sitemapLocation": "sitemap.json",
